package collection;



import java.util.Arrays;
import java.util.List;

public class list_convert {

	public static void main(String[] args) {
		String[] a= {"Ragul","Saravana","Senthil","Kavin","Lajeem","Dev"};
		List al=Arrays.asList("Payilagam Students");
		System.out.println(al);
		al.add(a);
		System.out.println(al);
		

	}

}
