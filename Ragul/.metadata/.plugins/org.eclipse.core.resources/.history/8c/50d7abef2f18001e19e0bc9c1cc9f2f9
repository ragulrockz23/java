package collection;

import java.util.ArrayList;
import java.util.Iterator;
import java.util.List;

public class Rotate_list {

	public static void main(String[] args) {
		List l=new ArrayList();
		l.add(1);
		l.add(2);
		l.add(3);
		l.add(4);
		l.add(5);
		System.out.println(l);
		Iterator itr=l.iterator();
		while(itr.hasNext()) {
			if((int)itr.next()==5) {
			itr.remove();
			}
		}
		System.out.println(l);

	}

}
